{
  "manifest_version": 3,
  "name": "Calculator",
  "version": "1.0",
  "description": "Calculate Anywhere",
  "action": {
    "default_icon": "hello_extensions.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none';"
  }
}