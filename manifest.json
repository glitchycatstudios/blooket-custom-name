{
  "manifest_version": 3,
  "name": "B-Namer",
  "version": "1.0",
  "description": "Choose your own name when random names is on in blooket.",
  "action": {
    "default_icon": "hello_extensions.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "clipboardWrite"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none';"
  } 
}